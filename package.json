{
  "name": "dietician-erp",
  "description": "A erp and lunch order system.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:legacy": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "npx prisma generate --schema ./prisma/postgres",
    "db:generate": "npx prisma generate --schema ./prisma/postgres",
    "db:push": "npx prisma db push --schema ./prisma/postgres",
    "db:migrate-dev": "npx prisma migrate dev --schema ./prisma/postgres",
    "db:migrate-deploy": "npx prisma migrate deploy --schema ./prisma/postgres",
    "db:reset": "npx prisma migrate reset --schema ./prisma/postgres --force",
    "db:seed": "npx prisma db seed --schema ./prisma/postgres",
    "db:studio": "npx prisma studio --schema ./prisma/postgres",
    "seed:create-admin": "tsx scripts/seeds/create-admin-account.ts",
    "seed:create-test": "tsx scripts/seeds/create-test-account.ts"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^6.17.0",
    "@prisma/client": "^6.15.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/crypto-js": "^4.2.2",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.4.1",
    "crypto-js": "^4.2.0",
    "daisyui": "^5.2.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "next": "15.5.7",
    "next-auth": "5.0.0-beta.29",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "swr": "^2.3.6",
    "word-list": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^16.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}